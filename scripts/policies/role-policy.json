{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "ec2:*",
                "iam:*",
                "autoscaling:*",
                "elasticloadbalancing:*",
                "s3:*",
                "dynamodb:*",
                "ssm:GetParameter"
            ],
            "Resource": "*"
        }
    ]
}